@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .

# Plugin type. As this plugin doesn't do anything useful, it doesn't have
# subtype
<urn:yru-rust-lv2-plugins:yru-flanger-rs-mono>
	a lv2:Plugin,
		lv2:ModulatorPlugin ,
			lv2:FlangerPlugin ;
# Associated project
# Plugin name
	doap:name "Yru Flanger Rs (Mono)" ;
	doap:license <https://www.gnu.org/licenses/gpl-3.0> ;
# Plugin version
	lv2:minorVersion 2 ;
	lv2:microVersion 2 ;
# Plugin features
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:optionalFeature lv2:isLive ;
#Author info
	doap:maintainer [
		foaf:name "Amaury ABRIAL aka Yruama_Lairba" ;
		foaf:mbox <yruama_lairba@hotmail.com>;
		foaf:homepage <https://github.com/YruamaLairba> ;
	] ;
# ports description
	lv2:port [
		a lv2:InputPort ,
			lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In" ;
	] , [
		a lv2:OutputPort ,
			lv2:AudioPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Out" ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "delay" ;
		lv2:name "Delay" ,
			"Délai"@fr ;
		units:unit   units:ms;
		lv2:default 1.8 ;
		lv2:minimum 0 ;
		lv2:maximum 10 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "depth" ;
		lv2:name "Depth" ,
			"Profondeur"@fr ;
		units:unit   units:coef;
		lv2:default 0.33 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "rate" ;
		lv2:name "Rate" ,
			"Vitesse"@fr ;
		units:unit   units:hz;
		lv2:default 0.40 ;
		lv2:minimum 0 ;
		lv2:maximum 20 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "feedback" ;
		lv2:name "Feedback" ,
			"Rétroaction"@fr ;
		units:unit   units:coef;
		lv2:default 0;
		lv2:minimum -1.0 ;
		lv2:maximum 1.0 ;
	] .

