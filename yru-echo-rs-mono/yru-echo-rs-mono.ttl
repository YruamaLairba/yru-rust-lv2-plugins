@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .

# Plugin type. As this plugin doesn't do anything useful, it doesn't have
# subtype
<urn:yru-rust-lv2-plugins:yru-echo-rs-mono>
	a lv2:Plugin ;
# Associated project
	lv2:project <https://github.com/YruamaLairba/yru-rust-lv2-plugins>;
# Plugin name
	doap:name "Yru Echo Rs (Mono)" ;
	doap:license <https://www.gnu.org/licenses/gpl-3.0> ;
# ports description
	lv2:port [
		a lv2:InputPort ,
			lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In" ;
	] , [
		a lv2:OutputPort ,
			lv2:AudioPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Out" ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "delay" ;
		lv2:name "Delay" ,
			"Délai"@fr ;
		units:unit   units:ms;
		lv2:default 100 ;
		lv2:minimum 0 ;
		lv2:maximum 2000 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "feedback" ;
		lv2:name "Feedback" ,
			"Rétroaction"@fr ;
		units:unit   units:coef;
		lv2:default 0.5 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "mix" ;
		lv2:name "Mix" ,
			"Mélange"@fr ;
		units:unit   units:coef;
		lv2:default 0.5 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] .

